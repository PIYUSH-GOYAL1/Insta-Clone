<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1><strong><em>Latest Feeds : </em></strong></h1>
    <% posts.reverse().forEach(function(elem){  %>
        <img src="/images/uploads/<%= elem.user.dp %>" alt="" style="width: 20px; height: 20px;"><p><%= elem.user.username %></p>
        <a href="/images/uploads/<%= elem.postimage %>"><img src="/images/uploads/<%= elem.postimage %>" alt="" style="width: 50px; height: 50px;"></a>
        <p><%= elem.posttext %> <%= elem.likes.length %> likes</p>
   <% }) %>

    <a href="/profile"><p><em>Go to Profile Page</em></p></a>
</body>
</html>